<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_product_template_eltete_form" model="ir.ui.view">
        <field name="name">product.template.eltete.form</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='general_information']" position="after">
                <page string="Atributos">
                    
                    <group colspan="2" >
                        <field name='ala_1' />
                        <field name='base' />
                        <field name='ala_2' />
                        <field name='grosor' />
                        <field name='longitud' />
                        <field name='alas' />
                        <field name='interior' />
                        <field name='peso' />
                    </group>

                </page>
            </xpath>
            
        </field>
    </record>
    
    
    <record id="view_product_category_form_eltete" model="ir.ui.view">
        <field name="name">product.category.form.eltete</field>
        <field name="model">product.category</field>
        <field name="inherit_id" ref="product.product_category_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='parent_id']" position="after">
                <field name="product_type" class="oe_inline"/>
                <field name="is_cantonera" class="oe_inline"/>
                <field name="is_perfilu" class="oe_inline"/>
                
            </xpath>
        </field>
    </record>
    
    
    
    <!-- ############################################
                 product.caracteristica.color
        ############################################# -->
    
    
        <record model="ir.ui.view" id="view_product_caracteristica_color_tree">
            <field name="name">product.caracteristica.color.tree</field>
            <field name="model">product.caracteristica.color</field>
            <field name="priority" eval="16"/>
            <field name="arch" type="xml">
                <tree string="Producto">
                    <field name="number"/>
                    <field name="name"/>
                    <field name="description"/>
                </tree>
            </field>
        </record>
        
        
        <record model="ir.ui.view" id="view_product_caracteristica_color_form">
            <field name="name">product.caracteristica.color.form</field>
            <field name="model">product.caracteristica.color</field>
            <field name="arch" type="xml">
                <form string="Color">
                    <sheet>
                        <group>
                            <field name="number"/>
                            <field name="name"/>
                            <field name="description"/>
                        </group>
                        
                    </sheet>
                </form>
            </field>
        </record>
        
        
        <record model="ir.ui.view" id="view_product_caracteristica_color_search">
            <field name="name">product.caracteristica.color.search</field>
            <field name="model">product.caracteristica.color</field>
            <field name="arch" type="xml">
                <search string="Color">
                    <field name="number"/>
                    <field name="name"/>
                    <field name="description"/>
                </search>
            </field>
        </record>

				
        <record model="ir.actions.act_window" id="action_product_caracteristica_color">
            <field name="name">Color</field>
            <field name="res_model">product.caracteristica.color</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        
        
        <menuitem name="Color" parent="stock.menu_product_in_config_stock" id="menu_product_caracteristica_color" action="action_product_caracteristica_color" sequence="20"/>
        
        
